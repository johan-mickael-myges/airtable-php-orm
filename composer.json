{
    "name": "johan-mickael/airtable-php-orm",
    "description": "Airtable ORM bundle for Symfony",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "symfony/http-client": "7.2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Airtable\\ORM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Airtable\\ORM\\Tests\\": "tests/"
        }
    }
}
